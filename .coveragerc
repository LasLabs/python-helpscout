[run]

branch = True

[report]

exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:

ignore_errors = True

include =
    */helpscout/*

omit =
    */virtualenv/*
    */tests/*
    setup.py
    tests.py

[xml]

output = coverage.xml
